{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "strict": ["error", "global"],
      "curly": "warn"
    }
}