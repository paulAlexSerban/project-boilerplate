{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "strict": ["error", "global"],
      "curly": "warn"
    }
}

